{
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "prettier.semi": false,
  "prettier.singleQuote": true,
  "cSpell.words": [
    "and",
    "bligewater",
    "demacia",
    "description",
    "freljord",
    "jsonify",
    "levelup",
    "levelup description",
    "mongodb",
    "noxus",
    "piltover",
    "piltover and zaun",
    "pymongo",
    "targon",
    "zaun"
  ]
}
